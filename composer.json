{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor-plugin/youtube",
                "version": "2.1.10",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/youtube/releases/youtube_2.1.10.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "vcs",
            "url": "https://github.com/rlmumford/common.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:rlmumford/composer-patches.git"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "ckeditor-plugin/youtube": "^2.1",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "dev-dev-relative-patches as 1.6",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/bootstrap": "^3.11",
        "drupal/ckeditor_youtube": "^1.1",
        "drupal/commerce": "^2.10",
        "drupal/commerce_braintree": "^1.0",
        "drupal/commerce_paypal": "^1.0@beta",
        "drupal/commerce_square": "^1.2",
        "drupal/communication": "1.x-dev",
        "drupal/console": "dev-master",
        "drupal/console-core": "dev-master as 1.9.2",
        "drupal/core": "^8.7",
        "drupal/cshs": "1.x-dev",
        "drupal/decoupled_auth": "^1.0@alpha",
        "drupal/elasticsearch_aws_connector": "6.x-dev",
        "drupal/elasticsearch_connector": "6.x",
        "drupal/email_registration": "^1.0@RC",
        "drupal/eu_cookie_compliance": "^1.2",
        "drupal/facets": "^1.4",
        "drupal/fontawesome": "^2.2",
        "drupal/geocoder": "^2.0@beta",
        "drupal/geofield": "^1.0@beta",
        "drupal/google_analytics": "^2.3",
        "drupal/legal": "^1.0@beta",
        "drupal/mailgun": "^1.0@RC",
        "drupal/material_base": "^1.5",
        "drupal/metatag": "^1.7",
        "drupal/og": "^1.0@alpha",
        "drupal/panelizer": "^4.1",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.3",
        "drupal/profile": "^1.0@RC",
        "drupal/recaptcha": "^2.3",
        "drupal/redirect": "^1.3",
        "drupal/search_api": "^1.14",
        "drupal/select2": "^1.0@beta",
        "drupal/seo_checklist": "^4.1",
        "drupal/social_media": "^1.3",
        "drupal/time_field": "^1.11",
        "drupal/token": "^1.5",
        "drupal/xmlsitemap": "^1.0@alpha",
        "drupal/yoast_seo": "^1.4",
        "drush/drush": "^9.0.0",
        "npm-asset/select2": "^4.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "rlmumford/common": "dev-master",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-core-require-dev": "8.7.x",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/devel": "^1.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "enable-patching": true,
        "patches": {
            "drupal/core": {
                "Layout builder context hook": "https://www.drupal.org/files/issues/2019-05-10/3001188-5.patch",
                "Layout builder visibility rules": "https://www.drupal.org/files/issues/2019-05-03/2916876-52.patch",
                "Layout builder form blocks": "https://www.drupal.org/files/issues/2019-08-12/3045171-79.patch",
                "Entity reference views autocomplete filters": "https://www.drupal.org/files/issues/2019-05-07/generic_entityreference_filter_22429699-162.patch"
            },
            "drupal/cshs": {
                "Compatability with entityreference filter": "./patches/cshs-entityreference_views_handler_compat.patch"
            },
            "drupal/inline_entity_form": {
                "Bundle selection fix": "https://www.drupal.org/files/issues/2019-08-20/3076005-2.patch"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/modules/rlmumford": ["rlmumford/common"],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "installer-types": ["bower-asset", "npm-asset"]
    }
}
